<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>


    <style name="FullScreenVideoTheme" parent="AppTheme">
        <item name="windowNoTitle">false</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="NumberStytle">
        <item name="android:layout_width">65dp</item>
        <item name="android:layout_height">65dp</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">20sp</item>
        <item name="android:background">@drawable/shape_circle</item>
        <item name="android:onClick">numberClick</item>
        <item name="android:layout_margin">6dp</item>

    </style>



</resources>
